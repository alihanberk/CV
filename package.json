{
  "name": "cv-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.10.4",
    "i18next": "^12.0.0",
    "i18next-browser-languagedetector": "^2.2.3",
    "i18next-xhr-backend": "^1.5.1",
    "node-sass-chokidar": "^1.3.4",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-i18next": "^8.3.6",
    "react-scripts": "2.1.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "sh -ac '. ./env/debug; npm-run-all -p watch-css start-js '",
    "build-js": "react-scripts build",
    "build:prod": "sh -ac '. ./env/production; npm-run-all build-css build-js'",
    "build:dev": "sh -ac '. ./env/development; npm-run-all build-css build-js'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "description": "Alihan Berk BilgiÃ§",
  "main": "index.js",
  "devDependencies": {
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alihanberk/CV.git"
  },
